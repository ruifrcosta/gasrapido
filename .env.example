# =============================================================================
# GasRápido - Arquivo de Configuração de Ambiente de Exemplo
# =============================================================================
# 
# INSTRUÇÕES:
# 1. Copie este arquivo para .env para produção
# 2. Copie este arquivo para .env.local para desenvolvimento
# 3. Preencha com suas credenciais reais
# 4. NUNCA commite arquivos .env com credenciais reais
#
# =============================================================================

# === AMBIENTE ===
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=development
EXPO_PUBLIC_APP_ENV=development

# === SUPABASE - CORE (OBRIGATÓRIO) ===
# URL do projeto Supabase
SUPABASE_URL=https://seu-projeto.supabase.co
NEXT_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co
EXPO_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co

# Chave anônima (pública) do Supabase
SUPABASE_ANON_KEY=sua_chave_anonima_supabase
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua_chave_anonima_supabase
EXPO_PUBLIC_SUPABASE_ANON_KEY=sua_chave_anonima_supabase

# Chave de serviço (privada) do Supabase - APENAS SERVIDOR
SUPABASE_SERVICE_ROLE_KEY=sua_chave_service_role_supabase
SUPABASE_KEY=sua_chave_service_role_supabase

# JWT Secret para validação de tokens
JWT_SECRET=super-secret-jwt-token-with-at-least-32-characters-long

# === CONFIGURAÇÃO DO BANCO DE DADOS ===
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres
DB_HOST=localhost
DB_PORT=54322
DB_NAME=gasrapido
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# === AUTENTICAÇÃO E SEGURANÇA ===
# Configurações de sessão
SESSION_SECRET=sua-chave-secreta-de-sessao-muito-longa-e-segura
AUTH_SECRET=sua-chave-secreta-de-auth-muito-longa-e-segura

# Configurações de CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:19006
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:19006

# === APLICAÇÕES WEB E MOBILE ===
# URLs das aplicações
NEXT_PUBLIC_WEB_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_API_URL=http://localhost:3000/api

# Configuração do PWA
NEXT_PUBLIC_PWA_ENABLED=true

# === GEOLOCALIZAÇÃO E MAPAS ===
MAPS_PROVIDER=google
GOOGLE_MAPS_API_KEY=sua_chave_api_google_maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=sua_chave_api_google_maps
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=sua_chave_api_google_maps
MAPBOX_ACCESS_TOKEN=seu_token_acesso_mapbox
NEXT_PUBLIC_MAPBOX_TOKEN=seu_token_acesso_mapbox

# === NOTIFICAÇÕES PUSH ===
NOTIFICATIONS_PROVIDER=fcm

# Firebase Cloud Messaging
FCM_SERVER_KEY=sua_chave_servidor_fcm
FCM_PROJECT_ID=seu_project_id_fcm
FCM_PRIVATE_KEY=seu_private_key_fcm
FCM_CLIENT_EMAIL=seu_email_cliente_fcm
NEXT_PUBLIC_FCM_PROJECT_ID=seu_project_id_fcm
EXPO_PUBLIC_FCM_PROJECT_ID=seu_project_id_fcm

# Web Push (VAPID keys)
VAPID_PUBLIC_KEY=sua_chave_publica_vapid
VAPID_PRIVATE_KEY=sua_chave_privada_vapid
VAPID_EMAIL=seu_email_vapid
NEXT_PUBLIC_VAPID_PUBLIC_KEY=sua_chave_publica_vapid

# === PAGAMENTOS ===
PAYMENT_PROVIDER=stripe

# Stripe
STRIPE_PUBLISHABLE_KEY=pk_test_sua_chave_publicavel_stripe
STRIPE_SECRET_KEY=sk_test_sua_chave_secreta_stripe
STRIPE_WEBHOOK_SECRET=whsec_seu_segredo_webhook_stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_sua_chave_publicavel_stripe
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_sua_chave_publicavel_stripe

# Multicaixa (Angola)
MULTICAIXA_API_KEY=sua_chave_api_multicaixa
MULTICAIXA_MERCHANT_ID=seu_id_merchant_multicaixa
MULTICAIXA_ENVIRONMENT=sandbox
MULTICAIXA_BASE_URL=https://api-sandbox.multicaixa.ao

# === CLIMA E TEMPO REAL ===
WEATHER_PROVIDER=openweather
OPENWEATHER_API_KEY=sua_chave_api_openweather
NEXT_PUBLIC_OPENWEATHER_API_KEY=sua_chave_api_openweather

# === COMUNICAÇÃO ===
COMMUNICATION_PROVIDER=twilio

# Twilio
TWILIO_ACCOUNT_SID=seu_account_sid_twilio
TWILIO_AUTH_TOKEN=seu_auth_token_twilio
TWILIO_PHONE_NUMBER=seu_numero_telefone_twilio
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# SendGrid
SENDGRID_API_KEY=sua_chave_api_sendgrid
SENDGRID_FROM_EMAIL=noreply@gasrapido.com
SENDGRID_FROM_NAME=GasRápido

# === MONITORAMENTO E LOGS ===
MONITORING_PROVIDER=sentry

# Sentry
SENTRY_DSN=sua_dsn_sentry
NEXT_PUBLIC_SENTRY_DSN=sua_dsn_sentry
EXPO_PUBLIC_SENTRY_DSN=sua_dsn_sentry
SENTRY_ORG=gasrapido
SENTRY_PROJECT=gasrapido-web
SENTRY_ENVIRONMENT=development

# === ARMAZENAMENTO E UPLOADS ===
STORAGE_PROVIDER=supabase

# Cloudinary (alternativo)
CLOUDINARY_CLOUD_NAME=seu_cloud_name_cloudinary
CLOUDINARY_API_KEY=sua_chave_api_cloudinary
CLOUDINARY_API_SECRET=sua_chave_secreta_cloudinary
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=seu_cloud_name_cloudinary

# === AUTENTICAÇÃO MULTIFATOR ===
MFA_PROVIDER=authenticator
TOTP_SECRET_KEY=sua_chave_secreta_totp
MFA_ISSUER=GasRápido

# === CACHE E SESSÕES ===
# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# === MESSAGE QUEUE ===
# RabbitMQ
RABBITMQ_URL=amqp://admin:admin@localhost:5672
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin

# === SERVICE DISCOVERY ===
# Consul
CONSUL_HTTP_ADDR=localhost:8500
CONSUL_HOST=localhost
CONSUL_PORT=8500

# === MONITORAMENTO AVANÇADO ===
# Prometheus
PROMETHEUS_URL=http://localhost:9090

# Grafana
GRAFANA_URL=http://localhost:3001
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200

# === BACKUP E REDUNDÂNCIA ===
BACKUP_PROVIDER=aws

# AWS S3
AWS_ACCESS_KEY_ID=sua_chave_acesso_aws
AWS_SECRET_ACCESS_KEY=sua_chave_secreta_aws
AWS_REGION=eu-west-1
AWS_S3_BUCKET=gasrapido-backups
BACKUP_ENCRYPTION_KEY_ID=alias/gasrapido-backup-key
BACKUP_BUCKET_PRIMARY=gasrapido-backups-eu-west-1
BACKUP_BUCKET_SECONDARY=gasrapido-backups-us-east-1
BACKUP_BUCKET_TERTIARY=gasrapido-backups-ap-southeast-1

# === CONTAINERIZAÇÃO ===
# Docker
DOCKER_REGISTRY=registry.gasrapido.com
DOCKER_USERNAME=seu_usuario_docker
DOCKER_PASSWORD=sua_senha_docker

# Kubernetes
KUBERNETES_CLUSTER_URL=https://kubernetes.gasrapido.com
KUBERNETES_TOKEN=seu_token_kubernetes
KUBERNETES_NAMESPACE=gasrapido

# === INTELIGÊNCIA ARTIFICIAL ===
AI_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=sua_chave_api_openai
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=1000

# === TICKETING E SUPORTE ===
TICKETING_PROVIDER=zendesk
TICKETING_WEBHOOK_URL=https://gasrapido.zendesk.com/api/v2/webhooks
ZENDESK_API_TOKEN=seu_token_api_zendesk
ZENDESK_EMAIL=suporte@gasrapido.com

# === ANALYTICS ===
# Google Analytics
NEXT_PUBLIC_GA_TRACKING_ID=UA-XXXXXXXXX-X
EXPO_PUBLIC_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Mixpanel
NEXT_PUBLIC_MIXPANEL_TOKEN=seu_token_mixpanel
EXPO_PUBLIC_MIXPANEL_TOKEN=seu_token_mixpanel

# === DESENVOLVIMENTO ===
# Debug
DEBUG=gasrapido:*
LOG_LEVEL=debug
VERBOSE=true

# Hot Reload
WATCHPACK_POLLING=true
NEXT_TELEMETRY_DISABLED=1

# === TESTES ===
# Configurações para testes
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:54322/gasrapido_test
TEST_SUPABASE_URL=http://localhost:54321
TEST_SUPABASE_ANON_KEY=sua_chave_test_supabase